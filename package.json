    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:promise/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "promise"
    ],
    "rules": {
      "no-console": "off",
      "no-undef": "off",
      "no-unused-vars": "off",
      "no-use-before-define": "off",
      "no-var": "error",
      "prefer-const": "error",
      "prefer-template": "error",
      "@typescript-eslint/await-thenable": "error"
    },
    "overrides": [
      {
        "files": [
          "src/**/*.spec.ts"
        ],
        "plugins": [
          "jest"
        ],
        "env": {
          "jest/globals": true
        }
      }
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "endOfLine": "lf",
    "trailingComma": "es5"
  },
  "stylelint": {
    "ignoreFiles": [
      "**/*"
    ],
    "overrides": [
      {
        "files": [
          "**/*.css"
        ],
        "extends": [
          "stylelint-config-standard"
